{
  "author": "Scott Smereka",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sieve",
      "aliases": ["Sieve", "sieve"],
      "extensions": [".sieve"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "sieve",
      "scopeName": "source.sieve",
      "path": "./syntaxes/sieve.tmLanguage.json"
    }]
  },
  "description": "Syntax highlighting for the Sieve email filtering language as specified by RFC 5228",
  "displayName": "Sieve",
  "engines": {
    "vscode": "^1.93.0"
  },
  "files": [
    "examples",
    "icon.png",
    "language-configuration.json",
    "LICENSE",
    "package.json",
    "syntaxes"
  ],
  "homepage": "https://github.com/ssmereka/vscode-sieve#readme",
  "icon": "icon.png",
  "keywords": [
    "email",
    "filtering",
    "highlighting",
    "rs5228",
    "syntax"
  ],
  "license": "BSD-2-Clause",
  "name": "vscode-sieve",
  "pricing": "Free",
  "publisher": "scottsmereka",
  "repository": {
    "type": "git",
    "url": "https://github.com/ssmereka/vscode-sieve"
  },
  "version": "0.0.4"
}
